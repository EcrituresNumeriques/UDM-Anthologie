{
  "connection": "default",
  "tableName": "user",
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "unique": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "minLength": 6
    },
    "resetPasswordToken": {
      "type": "string"
    },
    "sex": {
      "type": "integer"
    },
    "type": {
      "type": "string"
    },
    "tickets": {
      "collection": "ticket",
      "via": "createdBy"
    },
    "assignedTickets": {
      "collection": "ticket",
      "via": "assignedTo"
    },
    "comments": {
      "collection": "comment",
      "via": "createdBy"
    },
    "department": {
      "model": "department"
    },
    "assignedLaws": {
      "collection": "assignedLaw",
      "via": "user"
    },
    "histories": {
      "collection": "history",
      "via": "createdBy"
    },
    "organizations": {
      "collection": "organization",
      "via": "users"
    },
    "address": {
      "model": "address"
    }
  }
}
